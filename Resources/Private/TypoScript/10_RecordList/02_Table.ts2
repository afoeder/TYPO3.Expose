// REVIEWED for release

########################################
# TYPO3.Admin:RecordList.Table
# --------------------------------------
# Display a list of common records as table
########################################

prototype(TYPO3.Admin:RecordList.Table) < prototype(TYPO3.TypoScript:FluidRenderer) {
	templatePath = 'resource://TYPO3.Admin/Private/Elements/List/Table.html'
	tableBody = TYPO3.TypoScript:CollectionRenderer {
		collection = ${objects}
		itemName = 'item'
		itemRenderer = TYPO3.Admin:RecordList.Table.Row
	}
	objects = ${objects}
}